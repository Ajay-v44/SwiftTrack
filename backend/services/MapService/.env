# SwiftTrack Map Service Environment Variables

# Server Configuration
SERVER_PORT=8006

# Redis Cache Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Eureka Service Discovery
EUREKA_URL=http://127.0.0.1:8761/eureka/

# Nominatim Configuration (Geocoding)
# Public API (rate-limited to 1 req/sec)
NOMINATIM_URL=https://nominatim.openstreetmap.org
# Local instance (for production/high throughput)
NOMINATIM_LOCAL_URL=http://localhost:8088
NOMINATIM_USE_LOCAL=false

# OSRM Configuration (Routing)
# Public API
OSRM_URL=https://router.project-osrm.org
# Local instance
OSRM_LOCAL_URL=http://localhost:5000
OSRM_USE_LOCAL=false

# GraphHopper Configuration (Alternative Routing)
GRAPHHOPPER_URL=http://localhost:8989
GRAPHHOPPER_API_KEY=
GRAPHHOPPER_ENABLED=false

# Routing Engine Selection (osrm or graphhopper)
ROUTING_ENGINE=osrm

# Java Options
JAVA_OPTS=-Xms256m -Xmx512m
